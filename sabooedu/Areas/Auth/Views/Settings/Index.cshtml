@model IEnumerable<sabooedu.Areas.Auth.Models.Setting>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Auth/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create","",new{ @class="btn btn-space btn-primary"})
</p>
<table class="table table-striped table-hover table-fw-widget" id="table3">
 <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ContactPerson)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Contact)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.Whatsapp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.Logo)
        </th>
        <th></th>
    </tr>
	 </thead>
	 <tbody>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ContactPerson)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contact)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.Whatsapp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.Logo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Sid },new {@class= " badge badge-primary" }) |
            @Html.ActionLink("Details", "Details", new { id=item.Sid },new {@class= " badge badge-warning" }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Sid },new {@class= " badge badge-danger" })
        </td>
    </tr>
}
</tbody>
</table>
